verification_mode	= 1		#Enable comparison with MASA solution?
mode			= 'no_debug'	#To enable debug mode, use 'debug', anything else is normal mode.

[grid]

length			= 1.0		# Length of domain in each direction
dimension = 1 # dimension of domain
grid_points = 30 # Number of points in one direction


[solver]

thermal_conductivity 	= 1.0			# Thermal conductivity k_0	 
solver_name = gauss # Use either jacobi or gauss
order = 4 # Order of accuracy of stencil
error_TOL		= 0.00000000000000001	# Tolerance
max_iters		= 1000000		# Maximum number of iterations
